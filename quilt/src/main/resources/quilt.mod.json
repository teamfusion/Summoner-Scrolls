{
  "schema_version": 1,
  "mixin": [
    "summoner_scrolls.mixins.json",
    "summoner_scrolls-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "summoner_scrolls",
    "version": "${version}",
    "name": "Summoner Scrolls",
    "description": "Amongus Desc!",
    "authors": [
      "Team Fusion"
    ],
    "contact": {
      "sources": "https://github.com/teamfusion/Summoner-Scrolls",
      "issues": "https://github.com/teamfusion/Summoner-Scrolls/issues"
    },
    "license": [
      "MIT (Code)",
      "ARR (Assets)"
    ],
    "icon": "assets/summoner_scrolls/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "com.github.teamfusion.spyglassplus.quilt.SpyglassPlusQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18.2"
      },
      {
        "id": "architectury",
        "version": ">=4.9.83"
      }
    ]
  }
}