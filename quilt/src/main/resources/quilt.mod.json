{
  "schema_version": 1,
  "mixin": [
    "summonerscrolls.mixins.json",
    "summonerscrolls-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "summonerscrolls",
    "version": "${version}",
    "name": "Summoner Scrolls",
    "description": "Bringing more Magic into Minecraft!",
    "authors": [
      "Team Fusion"
    ],
    "contact": {
      "sources": "https://github.com/teamfusion/Summoner-Scrolls",
      "issues": "https://github.com/teamfusion/Summoner-Scrolls/issues"
    },
    "license": [
      "MIT (Code)",
      "ARR (Assets)"
    ],
    "icon": "assets/summonerscrolls/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "com.github.teamfusion.summonerscrolls.quilt.SummonerScrollsQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18.2"
      },
      {
        "id": "architectury",
        "version": ">=4.9.83"
      }
    ]
  }
}